{
    "name": "jundayw/laravel-sensitive",
    "description": "Laravel Sensitive Words Filter Package.",
    "keywords": ["laravel", "sensitive", "stop word", "filter"],
    "license": "MIT",
    "authors": [
        {
            "name": "jundayw",
            "email": "jundayw@126.com"
        }
    ],
    "require": {
        "php": ">=8.0",
        "illuminate/cache": ">=9.0",
        "illuminate/collections": ">=9.0"
    },
    "autoload": {
        "psr-4": {
            "Jundayw\\LaravelSensitive\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Jundayw\\LaravelSensitive\\SensitiveServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
